{
  "rewrites": [
    { "source": "/api/docs/swagger.json", "destination": "/docs/swagger.json" },
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/assets/(.*)", "destination": "/frontend/dist/assets/$1" },
    { "source": "/favicon.svg", "destination": "/frontend/dist/favicon.svg" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
